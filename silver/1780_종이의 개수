//https://www.acmicpc.net/problem/1780

//#include<stdio.h>
#include<iostream>
using namespace std;

int N;
int A[2222][2222];
int ans[3];
void f(int x,int y, int len){
    for(int i=0;i<len;i++){
        for(int j=0;j<len;j++){
            if(A[x+i][y+j]!=A[x][y]){
                for(int a = 0;a<3;a++){
                    for(int b=0;b<3;b++){
                        f(x+len/3*a,y+len/3*b,len/3);
                    }
                }
                return;
            }
        }
    }
    ans[A[x][y]+1]++;
}
int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    cin>>N;
    for(int i=0;i<N;i++){
        for(int j=0;j<N;j++){
            cin>>A[i][j];
        }
    }
    f(0,0,N);
    for(int i=0;i<3;i++){
        cout<<ans[i]<<"\n";
    }
    return 0;
}
